					<Headers>
						<Header key="X-CMC_PRO_API_KEY" pid="3"></Header>
					</Headers>
				</Request>
				<Response statusCode="100">
					<Content pid="200"></Content>
				</Response>
			</Connection>
		</Session>
	</HTTP>

